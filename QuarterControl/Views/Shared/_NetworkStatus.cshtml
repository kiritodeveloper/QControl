<div class="row">
    @if (Model.networkStatus)
    {
        <h5 class="text-center">
            <font color="green"><span class="glyphicon glyphicon-ok-circle"></span>&nbsp;CONECTADO</font>
        </h5>
    }
    else
    {
        <h5 class="text-center">
            <font color="red"><span class="glyphicon glyphicon-remove-circle"></span>&nbsp;NO CONECTADO</font>
        </h5>
    }

</div>